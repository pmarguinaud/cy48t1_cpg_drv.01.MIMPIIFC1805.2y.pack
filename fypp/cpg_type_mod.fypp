#:if use_gfl
USE YOM_YGFL, ONLY: TYPE_GFLD
USE TYPE_MODEL, ONLY : MODEL
#:endif

CLASS (CPG_SL${ind}$_TYPE)                         :: SELF
INTEGER (KIND=JPIM),                INTENT (IN)    :: BLOCK_INDEX
