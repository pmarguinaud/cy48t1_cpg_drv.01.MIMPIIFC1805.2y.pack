    #:endif
  ENDIF
#:endfor

#:if use_gfl
  #:for v in gfl.variables
    #:if v.array
    #:elif v.dim == 3
  IF (YGFL%Y${v.name}$%MP_SL1 > 0 .AND. YGFL%Y${v.name}$%MP1 > 0) THEN
    JFLD = YDTPTRSLB${ind}$%MSLB1GFL9  + (YGFL%Y${v.name}$%MP_SL1-1)*(SELF%NLEV+2)
    SELF%${v.name}$ => SELF%ZDATA (:, JFLD:JFLD+SELF%NLEV+1, BLOCK_INDEX)
  ELSE
